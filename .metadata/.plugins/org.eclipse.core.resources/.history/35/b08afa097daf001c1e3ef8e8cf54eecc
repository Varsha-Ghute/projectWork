package com.example.demo.entities;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="tbl_customers")
public class Customer 
{
	@Id
	int cid;
	
	@Column
	String cname;
	
	@Column
	String contactno;
	
	@Column
	String address;
	
	@Column
	int pincode;
	
	@Column
	String gender;
	
	@Column
	String aadharno;
	
	@Column
	int familymembers;
	
	@Column
	int loginnid;
}
